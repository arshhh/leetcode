class Solution {
public:
    void sortColors(int A[], int n) {
        // Start typing your C/C++ solution below
        // DO NOT write int main() function
        int n1=0;
    int n2=0;
	int n3=0;
	for(int i=0;i<n;i++){
		if(A[i]==0)
			n1++;
		else if(A[i]==1)
			n2++;
		else{
			n3++;
		}
	}
	for(int i=0;i<n;i++){
		if(i<n1)
			A[i]=0;
		else if(i>=n1 && i<n1+n2)
			A[i]=1;
		else 
			A[i]=2;

	}
    }
};

