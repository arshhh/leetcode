class Solution {
public:
    int removeElement(int A[], int n, int elem) {
        // Start typing your C/C++ solution below
        // DO NOT write int main() function
           int end=n-1;
	int start=0;
	while(start<=end){
		if(A[start]==elem){
			swap(A[start],A[end]);
			end--;
		}else{
			start++;
		}
	}
	return end+1;
    }
};

