class Solution {
    public:
 int numTrees(int n) {
        // Start typing your Java solution below
        // DO NOT write main() function
    
         if(n==0)
    return 1;
	if(n==1)
		return 1;
	int* num=new int[n+1];
	num[0]=1;
	num[1]=1;
	//i is the number of 
	for(int i=2;i<=n;i++){
		// j is the root
		int tmp=0;
		for(int j=1;j<=i;j++){
			tmp+=( num[j-1]*num[i-j] );
		}
		num[i] = tmp;
	}
	return num[n];
    }
};

