int transition[][6]={
   {2,0,3,9,1,9},
   {2,9,3,9,9,9},
   {2,8,4,5,9,9},
   {4,9,9,9,9,9},
   {4,8,9,5,9,9},
   {7,9,9,9,6,9},
   {7,9,9,9,9,9},
   {7,8,9,9,9,9},
   {9,8,9,9,9,9}
};

class Solution {
public:
    int type(char c){
        if(c>='0' && c<='9') //number
            return 0;
        if(c==' ') //space
            return 1;
        if(c=='.') //digit
            return 2;
        if(c=='e') //e char
            return 3;
        if(c=='+' || c=='-')
            return 4;
        return 5;
    }

    bool isNumber(const char *s) {
        int state=0;
        while(*s){
            int t=type(*s);
            state=transition[state][t];
            if(state==9)
                return false;
            s++;
        }
        if(state==2 || state==4 || state==8 || state==7)
            return true;
        return false;
    }
};
