class Solution {
public:
    void merge(int A[], int m, int B[], int n) {
        // Start typing your C/C++ solution below
        // DO NOT write int main() function
    int i=0;
	int j=0;
        while(i<m && j<n){
		if(A[i]<=B[j]){
			i++;
}else{
//put B[j] into A
//move A first
for(int k=m-1;k>=i;k--)
	A[k+1]=A[k];
m++;
A[i]=B[j];
    j++;
    i++;
    }
	}//
    
	if(i==m && j<n){
		for(int k=j;k<n;k++,i++)
			A[i] = B[k];
	}
    }
};

